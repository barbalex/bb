{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": {
    "browser": true
  },
    "plugins": [
        "react"
    ],
    "rules": {
    "react/require-extension": "off",
    "linebreak-style": 0,
    "react/jsx-no-bind": 0,
    "comma-dangle": 0,
    "no-use-before-define": 0,
    "consistent-return": 0,
    "semi": [2, "never"],
    "quotes": [2, "single"],
    "strict": 0,
    "max-len": ["error", 120, 2, {"ignoreUrls": true}],
    "no-underscore-dangle": ["error", { "allow": ["_id", "_rev", "_attachments"] }],
    "no-param-reassign": ["error", { "props": false }],
    "react/require-extension": "off",
    "prefer-es6-class": 0
  }
}
